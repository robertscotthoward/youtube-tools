{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: main",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: main --pull",
            "type": "debugpy",
            "request": "launch",
            "args": ["--pull", "https://www.youtube.com/watch?v=deDg1MQh4_Y"],
            // "args": ["--pull", "https://www.youtube.com/@PrometheanAction/videos"],
            "program": "main.py",
            "console": "integratedTerminal"
        }
    ]
}